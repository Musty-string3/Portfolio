<!--いいねした一覧画面-->
<div class="container">
  <h4 class="text-center">あなたがいいねした投稿一覧</h4>
  <% if @posts.present? %>
    <%= render 'public/posts/index', posts: @posts %>
  <% else %>
    <h5 class="text-center pt-3">
      いいねした投稿はまだありません。
    </h5>
  <% end %>
  <%= render 'public/posts/new_btn' %>
</div>